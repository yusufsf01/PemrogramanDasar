#include <iostream>
using namespace std;

class number1 {
    private:
    int y;

    public:
    void setNumber(int a) {
        y = a ;
    }
    int getNumber (){
        return y;
    }
};
class number2 {
    public:
    int x;
};

int main () {
    number1 satu;
    number2 dua;
    satu.setNumber(25);
    dua.x =30;

    cout << "y= " << satu.getNumber() << endl;
    cout << "x= " << dua.x << endl;

    return 0;
}
